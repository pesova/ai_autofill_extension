{
  "manifest_version": 3,
  "name": "Job Form Autofill",
  "version": "1.1.4",
  "description": "Autofill job application forms with saved data",
  "permissions": ["activeTab", "scripting", "storage"],
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [{
    "resources": ["config.json", "userData.json", "mocks.json"],
    "matches": ["<all_urls>"]
  }]
}