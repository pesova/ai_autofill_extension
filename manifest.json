{
  "manifest_version": 3,
  "name": "Job Form Autofill",
  "version": "1.1",
  "description": "Autofill job application forms with saved data",
  "permissions": ["activeTab", "scripting"],
  "icons": {
    "128": "icon.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}